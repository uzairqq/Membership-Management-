@using Membersips.Areas.Admin.Models
@model IEnumerable<Membersips.Models.Part>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.Partial("_CreateButtonPartialView")
</p>
<table class="table table-striped">
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
        @Html.Partial("_TableButtonPartial",new SmallButtonModel
        {
            Id = item.Id
        })
        </td>
    </tr>
}

</table>
